    }
  },
  "files": [
    "dist",
    "README.md",
    "screenshot.png"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "@mdi/js": "^7.4.47",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.1",
    "vite": "^3.0.0",
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  }
}
